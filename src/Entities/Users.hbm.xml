<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.Users" table="USERS" schema="IRISDREI">
        <id name="userid">
            <column name="USERID" sql-type="number(11)" precision="11"/>
        </id>
        <property name="username">
            <column name="USERNAME" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="firstName">
            <column name="FIRST_NAME" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="lastName">
            <column name="LAST_NAME" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="dateOfBirth">
            <column name="DATE_OF_BIRTH" sql-type="timestamp(6)" not-null="true"/>
        </property>
        <property name="registrationDate">
            <column name="REGISTRATION_DATE" sql-type="timestamp(6)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>