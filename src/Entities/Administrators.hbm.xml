<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.Administrators" table="ADMINISTRATORS" schema="IRISDREI">
        <id name="adminid">
            <column name="ADMINID" sql-type="number(11)" precision="11"/>
        </id>
        <property name="username">
            <column name="USERNAME" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>